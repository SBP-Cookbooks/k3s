<% unless @config['disable'].empty? -%>
disable:
<% @config['disable'].each do |option| -%>
  - "<%= option %>"
<% end -%>
<% end -%>
<% if @config['disable_cloud_controller']-%>
disable-cloud-controller: true
<% end -%>
<% unless @config['kubelet_args'].empty? -%>
kubelet-arg:
<% @config['kubelet_args'].each do |option| -%>
  - "<%= option %>"
<% end -%>
<% end -%>
<% unless @config['node_labels'].empty? -%>
node-label:
<% @config['node_labels'].each do |option| -%>
  - "<%= option %>"
<% end -%>
<% end -%>
<% unless @config['tls_san'].empty? -%>
tls-san:
<% @config['tls_san'].each do |option| -%>
  - "<%= option %>"
<% end -%>
<% end -%>
write-kubeconfig-mode: "<%= @config['write_kubeconfig_mode'] %>"
